{
    "name": "Meet Attendance",
    "version": "1.0.0",
    "description": "Get attendance of students in g-meet",
    "manifest_version": 2,
    "author": "Mishal Faisal",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup":"popup.html"
    },
    "permissions":[
        "activeTab",
        "clipboardWrite",
        "tabs"
    ]
}